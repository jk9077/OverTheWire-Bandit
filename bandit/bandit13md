-rw-r-----   1 bandit13 bandit12 2639 Jul 28 19:03 data.txt
-rw-r--r--   1 root     root      807 Mar 31  2024 .profile
bandit12@bandit:~$ mkdir /tmp/tmptmp
bandit12@bandit:~$ cp ./data.txt /tmp/tmptmp
bandit12@bandit:/tmp/tmptmp$ xxd -r ./data.txt
